package com.qst.chapter06.exercise02;

public class Pdemo {
private String a,b;
public Pdemo(String a,String b)
{
	this.a=a;
	this.b=b;
	}
public String getA() {
	return a;
}
public void setA(String a) {
	this.a = a;
}
public String getB() {
	return b;
}
public void setB(String b) {
	this.b = b;
}


}
